#include<iostream>
using namespace std;
int main(){
int a;
float b;
char c;
bool d;
double e;
cout<<"The size of integer datatype is "<<sizeof(a)<<endl;
cout<<"The size of float datatype is "<<sizeof(b)<<endl;
cout<<"The size of char datatype is "<<sizeof(c)<<endl;
cout<<"The size of bool datatype is "<<sizeof(d)<<endl;
cout<<"The size of double datatype is "<<sizeof(e)<<endl;

return 0;
}
